public class NewStuff {
	public static void main(String args[]) {
		int x=10, y=20, z=30;
		if (x<y) {
		System.out.println("Washington");
		} else if ((x+y)%2==0) {
		System.out.println("Adams");
		} else if (z-2 < y+1) {
		System.out.println("Jefferson");
		} else {
		System.out.println("Madison");
		}
		}
}